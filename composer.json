{
  "name": "solital/log",
  "license": "MIT",
  "description": "PSR-3 Logger library for Solital Framework",
  "keywords": [
    "phoole",
    "library",
    "php",
    "logger",
    "psr-3",
    "swoole"
  ],
  "require": {
    "php": "^8.0",
    "phoole/base": "^1.0.20",
    "psr/log": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Solital\\Core\\Logger\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Solital\\Core\\Logger\\Tests\\": "tests/"
    }
  },
  "provide": {
    "psr/log-implementation": "1.0.0"
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit",
    "check": "phpcs --standard=psr12 src/",
    "format": "phpcbf --standard=psr12 src/"
  }
}